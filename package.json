{
  "name": "@yankeeinlondon/meta-builder",
  "version": "1.0.2",
  "description": "Adds better metadata management capabilities to vite-plugin-md via the Builder API",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format=esm,cjs --dts --clean",
    "watch": "tsup src/index.ts --format=esm,cjs --dts --watch",
    "lint": "run-p lint:*",
    "lint:src": "eslint ./src/**/*.ts --fix",
    "lint:test": "eslint ./test/**/*.ts --fix",
    "test": "vitest",
    "test:ci": "vitest run",
    "test:ui": "vitest ui",
    "audit:fix": "pnpm audit --fix",
    "release": "run-s lint release:latest test:ci audit:fix release:bump",
    "release:latest": "pnpm install",
    "release:bump": "bumpp"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yankeeinlondon/meta-builder.git"
  },
  "keywords": [
    "markdown",
    "vue",
    "vitejs",
    "builder",
    "vite-plugin-md"
  ],
  "author": "Ken Snyder <ken@ken.net>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yankeeinlondon/meta-builder/issues"
  },
  "homepage": "https://github.com/yankeeinlondon/meta-builder#readme",
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.2",
    "@antfu/ni": "^0.17.2",
    "@types/node": "^16.11.45",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "@vitejs/plugin-vue": "^3.0.1",
    "@vitest/ui": "^0.18.1",
    "@vue/test-utils": "^2.0.2",
    "bumpp": "^8.2.1",
    "c8": "^7.12.0",
    "callsites": "^4.0.0",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-unicorn": "^43.0.2",
    "esno": "^0.16.3",
    "happy-dom": "^6.0.4",
    "markdown-it": "^13.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "prismjs": "^1.28.0",
    "tsup": "^6.1.3",
    "typescript": "^4.7.4",
    "vite": "^3.0.2",
    "vite-plugin-md": "^0.19.7",
    "vitest": "^0.18.1",
    "vue": "^3.2.37"
  },
  "dependencies": {
    "@yankeeinlondon/builder-api": "^0.3.0"
  }
}
